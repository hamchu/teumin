         <children>
            <ImageView fx:id="img" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" smooth="false" />
         </children>
      </Pane>
      <Button layoutX="130.0" layoutY="233.0" mnemonicParsing="false" onMouseClicked="#click_ok" text="완료" />
   </children>
</AnchorPane>
